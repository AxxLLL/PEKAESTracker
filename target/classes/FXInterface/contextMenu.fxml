<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<ContextMenu fx:id="contextMenu" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContextMenuController">
  <items>
      <Menu mnemonicParsing="false" text="Unspecified Menu">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#copyShippingNumber" text="Kopiuj numer przesyłki" />
        </items>
      </Menu>
      <MenuItem mnemonicParsing="false" onAction="#refreshShipmentStatus" text="Odśwież status" />
      <SeparatorMenuItem mnemonicParsing="false" />
      <MenuItem mnemonicParsing="false" onAction="#deleteShipmentFromList" text="Usuń z listy" />
  </items>
</ContextMenu>
